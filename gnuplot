set terminal qt font "Droid Sans, 12" enhanced
set encoding default
set style data lines

## line styles
set linetype 1  linecolor rgb "blue" pointsize 1 pointtype 7
set linetype 2  linecolor rgb "red" pointsize 1 pointtype 9
set linetype 3  linecolor rgb "dark-green" pointsize 1 pointtype 5
set linetype 4  linecolor rgb "dark-violet" pointsize 1
set linetype 5  linecolor rgb "dark-orange" pointsize 1
set linetype 6  linecolor rgb "brown" pointsize 1
set linetype 7  linecolor rgb "dark-pink" pointsize 1
set linetype 8  linecolor rgb "gold" pointsize 1
set linetype 9  linecolor rgb "dark-cyan" pointsize 1
set linetype 10 linecolor rgb "spring-green" pointsize 1

set style line 20 linecolor rgb "#cccccc"
set grid ls 20

set key top right width 1 height 1 box
